<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.luolh.mapper.TbUserMapper">
    <select id="findByUsername" resultType="com.luolh.entity.TbUser">
        select user_id, username, passwords
        from tb_user
        where username = #{username}
    </select>
    <insert id="insert">
        insert into tb_user(username, passwords)
        values(#{username},#{passwords})
    </insert>
</mapper>